// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql" //soalnya menggunakan mysql (bisa juga postgresql)
  url      = env("DATABASE_URL")
}

model Product {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  description String?
  location String
  image String
  label Int 
  price Int
}